{
    "name": "advicepharmagroup/mailup",
    "description": "Driver per invio email tramite api Mailup",
    "version": "1.0.0",
    "keywords": [
        "advicepharma",
        "mailup"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/advicepharmagroup/mailup",
    "authors": [
        {
            "name": "Flavio Maniscalco",
            "email": "flavio.maniscalco@advicepharma.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "Advicepharmagroup\\Mailup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Advicepharmagroup\\Mailup\\Provider\\MailupServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}